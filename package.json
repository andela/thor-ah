{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "engines": {
    "node": "10.8.0"
  },
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./ --fix",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "pretest": "NODE_ENV=test npm run migrate",
    "test": "NODE_ENV=test nyc mocha --compilers js:babel-register ./server/tests/ --timeout 5000ms --exit --no-deprecation",
    "start": "npm run build && node dist/index.js",
    "build": "rimraf dist/ && babel ./ -d dist/ --ignore node_modules --copy-files",
    "start:dev": "nodemon --watch server --exec babel-node ./index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "fancy-log": "^1.3.2",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pusher": "^2.1.3",
    "request": "^2.87.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "sequelize-test-helpers": "^1.0.4",
    "sinon": "^6.1.5",
    "slug": "^0.9.1",
    "swagger-ui-express": "^4.0.1",
    "underscore": "^1.9.1",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1"
  }
}
